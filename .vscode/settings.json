{
  "editor.formatOnSave": true,
  "editor.codeActionsOnSave": {
    "source.fixAll.tslint": true,
    "source.fixAll.eslint": true
  },
  "files.exclude": {
    "**/.cache": true,
    "**/.DS_Store": true,
    "**/.git": true,
    "**/.nyc_output": true,
    "**/.parcel-cache": true,
    "**/dist": true,
    "**/node_modules": true,
    "**/yarn.lock": true,
    "**/package-lock.json": true,
    "*.socket": true
  },
}